{
  "name": "@aws/amazon-location-for-maplibre-geocoder",
  "version": "1.0.0",
  "description": "Maplibre Plugin to Support Amazon Location Service Integration",
  "main": ".dist/cjs/index.js",
  "module": ".dist/esm/index.js",
  "types": ".dist/types/index.d.ts",
  "files": [
    "./package.json",
    "./dist"
  ],
  "scripts": {
    "clean": "rm -r dist",
    "prettier": "prettier -w .",
    "prettier:check": "prettier -c .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typedoc": "typedoc",
    "test": "jest --coverage",
    "build": "npm-run-all build:*",
    "build:ts": "npm-run-all build-ts:*",
    "build:bundle": "rollup -c",
    "build-ts:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build-ts:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build-ts:esm": "tsc --esModuleInterop --module esnext --outDir dist/esm",
    "prepare": "husky install",
    "prepublishOnly": "npm-run-all clean lint prettier:check test build"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@aws/amazon-location-utilities-auth-helper": "^1.0.2",
    "@aws/amazon-location-client" : "^1.x"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rollup": "^3.22.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.2"
  }
}

